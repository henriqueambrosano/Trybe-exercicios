#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/henriqueambrosano/Desktop/Trybe/Trybe-exercicios/back-end/secao-6-nodejs-orm-e-autenticacao/dia-3-orm-associations-n:n-e-transactions/aprendendo-na-pratica/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/henriqueambrosano/Desktop/Trybe/Trybe-exercicios/back-end/secao-6-nodejs-orm-e-autenticacao/dia-3-orm-associations-n:n-e-transactions/aprendendo-na-pratica/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../js-beautify@1.14.6/node_modules/js-beautify/js/bin/js-beautify.js" "$@"
else
  exec node  "$basedir/../../../../../js-beautify@1.14.6/node_modules/js-beautify/js/bin/js-beautify.js" "$@"
fi
